//print ****
  #include<stdio.h>
int main()
{
	int i;
	
	for (i = 0; i < 4; i++)
	{
		printf("*");
	}

	return(0);
  }
  
  
  
  
  //print **** **** ****  ****
       
#include<stdio.h>
int main()
{
	int i;

	for (i = 0; i < 4; i++)
	{
		printf("\n****");
	}

	return(0);
}




//print 1to 10 number

  #include<stdio.h>
  int main()
{
      int i;
      for (i = 1; i <= 10; i++)

      {
          printf("%d", i);
      }

  
      return(0);
  }





//print **** 12 times
  #include<stdio.h>
  int main()
{
   int i;
   for (i = 0; i < 12; i++)
   {
	   printf("\n****");


   }

	return(0);
}




//print 1to 10 number

  #include<stdio.h>
  int main()
{
      int i;
      for (i = 1; i <= 10; i++)

      {
          printf("\n%d", i);
      }

  
      return(0);
  }





//print addition of two no for 1 to 10   output-55

 #include<stdio.h>
  int main()
{
	int i ;
	int sum ;

	for (i = 1; i <= 10; i++)
	{
		sum = sum + i;
		printf("%d\n", i);
	}

	printf("addition of all no=%d\n",sum);

	return(0);
}


//print even no 1 to 10 using loop//
#include<stdio.h>
 
int main()
{

	int i;

	for (i = 2; i <=10; i = i + 2)

	{
		printf("\n%d", i);

	}


	return(0);
}





//print odd no between 1to 10
 #include<stdio.h>
 int main()
{
	 int i;
	 for (i = 1; i < 11;i=i+2)
	 {
		 printf("\n%d", i);
	 }





	return(0);
 }




//addition of all odd number between 1 to 100

#include<stdio.h>
int main()
{
	int i;
	int add;
	
	for (i = 1; i <= 100; i = i + 1)

	{
		add = add + i;
		printf("\n%d", i);
	}

	printf("\n%d", add);


	return(0);
}




//addition of all even no between 1 to 10

#include<stdio.h>
int main()
{
	int i;
	int add;
	for (i = 1; i < 10; i = i + 2)
	{
		add = add + i;
		printf("\n%d", i);

	}

	printf("\n%d", add);

	return(0);
}
